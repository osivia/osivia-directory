<?xml version="1.0" encoding="UTF-8"?>
<beans
    xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context"
    xmlns:tx="http://www.springframework.org/schema/tx"
    xsi:schemaLocation="http://www.springframework.org/schema/beans 
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-2.5.xsd">

   <bean id="foreignBeanProvider" class="org.osivia.services.directory.helper.PortletStarter" />
    
   <bean id="directoryServiceLocator" factory-bean="foreignBeanProvider" factory-method="getDirectoryServiceLocator" />
    
   <bean id="personne" class="fr.toutatice.outils.ldap.entity.Person" factory-bean="foreignBeanProvider" factory-method="getForeignBean">
        <constructor-arg type="java.lang.String" value="person" />
        <constructor-arg type="java.lang.Class" value="fr.toutatice.outils.ldap.entity.Person" />
    </bean>
    
    <bean id="profil" class="fr.toutatice.outils.ldap.entity.Profil" factory-bean="foreignBeanProvider" factory-method="getForeignBean">
        <constructor-arg type="java.lang.String" value="profil" />
        <constructor-arg type="java.lang.Class" value="fr.toutatice.outils.ldap.entity.Profil" />
    </bean>    
    
    <bean id="roleApplicatif" class="fr.toutatice.outils.ldap.entity.RoleApplicatif" factory-bean="foreignBeanProvider" factory-method="getForeignBean">
        <constructor-arg type="java.lang.String" value="roleApplicatif" />
        <constructor-arg type="java.lang.Class" value="fr.toutatice.outils.ldap.entity.RoleApplicatif" />
    </bean> 
    
    <bean id="structure" class="fr.toutatice.outils.ldap.entity.Structure" factory-bean="foreignBeanProvider" factory-method="getForeignBean">
        <constructor-arg type="java.lang.String" value="structure" />
        <constructor-arg type="java.lang.Class" value="fr.toutatice.outils.ldap.entity.Structure" />
    </bean>
    
     <bean id="etablissement" class="fr.toutatice.outils.ldap.entity.Etablissement" factory-bean="foreignBeanProvider" factory-method="getForeignBean">
        <constructor-arg type="java.lang.String" value="etablissement" />
        <constructor-arg type="java.lang.Class" value="fr.toutatice.outils.ldap.entity.Etablissement" />
    </bean>
    
    <bean id="organisation" class="fr.toutatice.outils.ldap.entity.Organisation" factory-bean="foreignBeanProvider" factory-method="getForeignBean">
        <constructor-arg type="java.lang.String" value="organisation" />
        <constructor-arg type="java.lang.Class" value="fr.toutatice.outils.ldap.entity.Organisation" />
    </bean>
    
     <bean id="organizationalUnit" class="fr.toutatice.outils.ldap.entity.OrganizationalUnit" factory-bean="foreignBeanProvider" factory-method="getForeignBean">
        <constructor-arg type="java.lang.String" value="organizationalUnit" />
        <constructor-arg type="java.lang.Class" value="fr.toutatice.outils.ldap.entity.OrganizationalUnit" />
    </bean>    

     <bean id="application" class="fr.toutatice.outils.ldap.entity.Application" factory-bean="foreignBeanProvider" factory-method="getForeignBean">
        <constructor-arg type="java.lang.String" value="application" />
        <constructor-arg type="java.lang.Class" value="fr.toutatice.outils.ldap.entity.Application" />
    </bean>
    
     <bean id="applicationMere" class="fr.toutatice.outils.ldap.entity.ApplicationMere" factory-bean="foreignBeanProvider" factory-method="getForeignBean">
        <constructor-arg type="java.lang.String" value="applicationMere" />
        <constructor-arg type="java.lang.Class" value="fr.toutatice.outils.ldap.entity.ApplicationMere" />
    </bean>
    
    <bean id="applicationFille" class="fr.toutatice.outils.ldap.entity.ApplicationFille" factory-bean="foreignBeanProvider" factory-method="getForeignBean">
        <constructor-arg type="java.lang.String" value="applicationFille" />
        <constructor-arg type="java.lang.Class" value="fr.toutatice.outils.ldap.entity.ApplicationFille" />
    </bean>
    
    <bean id="groupesApplications" class="fr.toutatice.outils.ldap.entity.GroupesApplications" factory-bean="foreignBeanProvider" factory-method="getForeignBean">
        <constructor-arg type="java.lang.String" value="groupesApplications" />
        <constructor-arg type="java.lang.Class" value="fr.toutatice.outils.ldap.entity.GroupesApplications" />
    </bean>     

</beans>
