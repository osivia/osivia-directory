<?xml version="1.0" standalone="yes"?>
<!DOCTYPE deployments PUBLIC "-//JBoss Portal//DTD Portlet Instances 2.6//EN" "http://www.jboss.org/portlet/dtd/portlet-instances_2_6.dtd">

<deployments>
	<deployment>
        <if-exists>overwrite</if-exists>
		<instance>
            <display-name xml:lang="en">Directory - Services</display-name>
			<display-name xml:lang="fr">Annuaire - Services</display-name>
			<instance-id>directory-services-instance</instance-id>
			<portlet-ref>directory-services</portlet-ref>
            <preferences>
                <preference>
                    <name>hide</name>
                    <value>true</value>
                </preference>
            </preferences>
		</instance>
	</deployment>
</deployments>